{
    "name": "norsys/score",
    "description": "A library to write composer's configuration file",
    "type": "project",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Frédéric Hardy",
            "email": "frederic.hardy@mageekbox.net"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "atoum/atoum": "@dev"
    },
    "autoload": {
        "psr-4": {
            "norsys\\score\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "norsys\\score\\tests\\units\\data\\provider\\": "tests/units/data/provider",
            "norsys\\score\\tests\\units\\": "tests/units/src"
        }
    }
}
